<Check@CheckBox>:
    group: 'group'
    size_hint: None, None
    size: dp(48), dp(48)
<Login>:
    email: campo_email_user
    senha: campo_senha_user
    #Titulo
    MDFloatLayout:
        MDLabel:
            text: 'Entrar'
            font_name: 'Fonts\Comfortaa-Regular.ttf'
            pos_hint: {'center_x': .55, 'center_y': .87}
            font_size: 36

    #Label 'E-mail'
    MDTextField:
        id: campo_email_user
        hint_text: "E-mail:"
        mode: 'rectangle'
        icon_left: 'email-outline'
        icon_left_color_normal: 0, 0, 0, .5
        icon_left_color_focus: 0, 0, 0, 1
        line_color_normal: 0, 0, 0, 1
        hint_text_color_normal: 0, 0, 0, .5
        text_color_normal: 0, 0, 0, 1
        line_color_focus: 0, 0, 0, 1
        hint_text_color_focus: 0, 0, 0, .5
        text_color_focus: 0, 0, 0, 1
        pos_hint: {'center_x': .50, 'center_y': .77}
        size_hint: .9, .08

    #Label 'Senha'
    MDTextField:
        id: campo_senha_user
        hint_text: "Senha:"
        mode: 'rectangle'
        password: True
        icon_left: 'lock-outline'
        icon_left_color_normal: 0, 0, 0, .5
        icon_left_color_focus: 0, 0, 0, 1
        line_color_normal: 0, 0, 0, 1
        hint_text_color_normal: 0, 0, 0, .5
        text_color_normal: 0, 0, 0, 1
        line_color_focus: 0, 0, 0, 1
        hint_text_color_focus: 0, 0, 0, .5
        text_color_focus: 0, 0, 0, 1
        pos_hint: {'center_x': .50, 'center_y': .68}
        size_hint: .9, .08
    MDIconButton:
        icon: 'eye-off'
        pos_hint: {'center_x': .857, 'center_y': .675}
        icon_size: '30sp'
        theme_icon_color: "Custom"
        icon_color: 0, 0, 0, .3
        on_release:
            self.icon = 'eye' if self.icon == 'eye-off' else 'eye-off'
            campo_senha_user.password = False if campo_senha_user.password is True else True

    #Botão 'Esqueceu Senha'
    MDTextButton:
        text: 'Esqueceu a senha?'
        pos_hint: {'center_x': .27, 'center_y': .57}
        font_style: 'H6'
        font_size: '18'

    #Botão 'Cadastre-se'
    MDTextButton:
        text: 'Cadastre-se'
        pos_hint: {'center_x': .77, 'center_y': .57}
        font_style: 'H6'
        font_size: '18'
        on_release:
            root.transition.direction = 'left'
            root.current = 'tela_cadastro'

    #Botão 'ENTRAR'
    MDRaisedButton:
        text: "ENTRAR"
        font_style: 'Button'
        md_bg_color: '#00000'
        pos_hint: {'center_x': .50, 'center_y': .40}
        size_hint: .9, .07    
        on_release:
            root.autentication()
                


MDScreenManager:
    id: scr_mmgr_autentication
    tela_entrar: tela_entrar
    #   TELA INICIAL
    MDScreen:
        md_bg_color: "#BDDBFF"
        name: 'tela_inicial'

        #Logo do App
        AsyncImage:
            source: "Images\logo.png"
            pos_hint:{"center_x": .5, "center_y": .6}
            size_hint: 0.8, 0.9

        #Botão 'Entrar'
        MDRaisedButton:
            text: 'ENTRAR'
            text_color: '#020202'
            font_style: "Button"
            md_bg_color: '#FFFFFF'
            line_color: '#000000'
            pos_hint: {"center_x": .25, "center_y": .15}
            size_hint: 0.4, 0.06
            on_release:
                root.transition.direction = 'left'
                root.current = 'tela_entrar'

        #Botão 'Cadastrar-se'
        MDRaisedButton:
            text: 'CADASTRAR-SE'
            text_color: '#FFFFFF'
            font_style: "Button"
            md_bg_color: '#3892E0'
            line_color: '#000000'
            pos_hint: {"center_x": .75, "center_y": .15}
            size_hint: 0.4, 0.06
            on_release:
                root.transition.direction = 'left'
                root.current = 'tela_cadastro'               
    
    #   TELA ENTRAR
    MDScreen:
        id: tela_entrar
        md_bg_color: "#BDDBFF"
        name: 'tela_entrar'

        #Botão 'Voltar'
        MDIconButton:
            icon: 'arrow-left-top'
            pos_hint: {'center_x': .06, 'center_y': .95}
            theme_icon_color: "Custom"
            icon_color: 0, 0, 0, .4
            on_release:
                root.transition.direction = 'right'
                root.current = 'tela_inicial'
        Login

    #   TELA DE CADASTRO
    MDScreen:
        md_bg_color: "#BDDBFF"
        name: 'tela_cadastro'

        #Botão 'Voltar'
        MDIconButton:
            icon: 'arrow-left-top'
            pos_hint: {'center_x': .06, 'center_y': .95}
            theme_icon_color: "Custom"
            icon_color: 0, 0, 0, .4
            on_release:
                root.transition.direction = 'right'
                root.current = 'tela_inicial'

        #Titulo
        MDFloatLayout:
            MDLabel:
                text: 'Cadastre-se'
                font_name: 'Fonts\Comfortaa-Regular.ttf'
                pos_hint: {'center_x': .55, 'center_y': .87}
                font_size: 36

            #Label 'Nome'        
            MDTextField:
                hint_text: "Nome:"
                mode: 'rectangle'
                icon_left: 'account-circle-outline'
                icon_left_color_normal: 0, 0, 0, .5
                icon_left_color_focus: 0, 0, 0, 1
                line_color_normal: 0, 0, 0, 1
                hint_text_color_normal: 0, 0, 0, .5
                text_color_normal: 0, 0, 0, 1
                line_color_focus: 0, 0, 0, 1
                hint_text_color_focus: 0, 0, 0, .5
                text_color_focus: 0, 0, 0, 1
                pos_hint: {'center_x': .25, 'center_y': .75}
                size_hint: .4, .08
            
            #Label 'Sobrenome'
            MDTextField:
                hint_text: "Sobrenome:"
                mode: 'rectangle'
                line_color_normal: 0, 0, 0, 1
                hint_text_color_normal: 0, 0, 0, .5
                text_color_normal: 0, 0, 0, 1
                line_color_focus: 0, 0, 0, 1
                hint_text_color_focus: 0, 0, 0, .5
                text_color_focus: 0, 0, 0, 1
                pos_hint: {'center_x': .75, 'center_y': .75}
                size_hint: .4, .08

            #Label 'Gênero'
            MDLabel:
                text: 'Gênero (Opcional)'
                pos_hint: {'center_x': .560, 'center_y': .67}
            Check:
                pos_hint: {'center_x': .10, 'center_y': .63}
                color: 'black'
            MDIcon:
                icon: 'human-male'
                pos_hint: {'center_x': .15, 'center_y': .60}
            
            Check:
                pos_hint: {'center_x': .30, 'center_y': .63}
