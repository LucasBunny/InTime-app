<Tarefa>:
    padding: 4
    size_hint: None, None
    size: "300dp", "150dp"
    pos_hint: {"center_x": .5, "center_y": .5}

    MDRelativeLayout:
        MDLabel:
            id: label_titulo
            text: root.titulo
            padding: [15, 15]
            color: "black"
            pos: "3dp", "45dp"
            bold: True

        MDLabel:
            id: label_data
            text: root.data
            color: "black"
            pos: "240dp", "45dp"
            bold: True

        MDLabel:
            id: label_texto
            text: root.text
            markup: True
            padding: [15, 15]
            color: "black"
            pos: "3dp", "-35dp"
        
<PomodoroAnimacao>
    canvas:
        Color:
            rgba: root.pomodoro_cor + [0.8]
        Line:
            width: root.pomodoro_tamanho
            ellipse: (self.x, self.y, self.width, self.height, 0, self.pomodoro_valor*3.6)


<Check@CheckBox>:
    group: 'group'
    size_hint: None, None
    size: dp(48), dp(48)

<Tela_Inicial@BackgroundColorBehavior>:
    md_bg_color: "#BDDBFF"
<Login@BackgroundColorBehavior>:
    md_bg_color: "#BDDBFF"
<Cadastrar@BackgroundColorBehavior>:
    md_bg_color: "#BDDBFF"
<Tela_Relatorios@BackgroundColorBehavior>:
