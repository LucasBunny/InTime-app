        pos_hint: {'center_x': .56, 'center_y': .74}
        font_size: 36

    #Menu de Navegação
    MDTopAppBar:
        elevation: 10
        pos_hint: {"down": 1}
        md_bg_color: "#FFFFFF"
        specific_text_color: "#4a4939"
        left_action_items:
            [['menu', lambda x: nav_drawer_relatorios.set_state("open")]]
    MDNavigationDrawer:
        id: nav_drawer_relatorios
        radius: 0, 0, 0, 0
        md_bg_color: "#7CB2F2"
        MDNavigationDrawerMenu:
            #Perfil do Usuário
            FitImage:
                size_hint: None, None
                size: "70dp", "70dp"
                source: "Images/dark souls.jpg"
                radius: 36, 36, 36, 36
            MDNavigationDrawerLabel:
                text: 'Nome/Sobrenome'
                padding: '12dp', '12dp', 0, '56dp'

            #Navegação entre telas
            MDNavigationDrawerItem:
                text: 'Início'
                icon: 'home'
                font_style: 'Button'
                on_release:
                    nav_drawer_relatorios.set_state('close')
                    root.manager.current = 'tela_relatorios'
            MDNavigationDrawerDivider:
            MDNavigationDrawerItem:
                text: 'Tarefas'
                icon: 'check-circle'
                font_style: 'Button'
                on_release:
                    nav_drawer_relatorios.set_state('close')
                    root.manager.current = 'tela_tarefas'
            MDNavigationDrawerDivider:
            MDNavigationDrawerItem:
                text: 'Lembretes'
